{
  "name": "koa-ts-frame",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/Vibing/koa-ts-frame.git",
  "author": "chenlong <cl903254852@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=production npx nodemon",
    "dev2": "cross-env NODE_ENV=production node app/index.js",
    "start": "NODE_ENV=production && pm2 start app/index.js --node-args='-r tsconfig-paths/register'",
    "tsc": "rm -rf dist && TS_NODE_PROJECT=tsconfig.json && tsc -p tsconfig.json",
    "build": "npx babel ./app ./bin ./configs ./expands --out-dir ./ --extensions \".js,.ts\"",
    "clean": "node bin/clean.js"
  },
  "dependencies": {
    "ali-rds": "^3.3.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.12.2",
    "dotenv": "^8.2.0",
    "glob": "^7.1.6",
    "kcors": "^2.2.2",
    "koa": "^2.12.0",
    "koa-bodyparser": "^4.3.0",
    "koa-multer": "^1.0.2",
    "koa-router": "^9.0.1",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.9.0-alpha.3",
    "typedi": "^0.8.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.3",
    "@babel/core": "^7.10.3",
    "@babel/node": "^7.10.3",
    "@babel/plugin-proposal-decorators": "^7.10.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.3",
    "@babel/plugin-proposal-optional-chaining": "^7.10.3",
    "@babel/preset-env": "^7.10.3",
    "@babel/preset-typescript": "^7.10.1",
    "@types/dotenv": "^8.2.0",
    "@types/kcors": "^2.2.3",
    "@types/koa": "^2.11.3",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-router": "^7.4.1",
    "@types/node": "^14.0.13",
    "babel-plugin-module-resolver": "^4.0.0",
    "cross-env": "^7.0.2",
    "nodemon": "^2.0.4",
    "ts-node": "^8.10.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.9.5"
  }
}
